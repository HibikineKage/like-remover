{
  "manifest_version": 2,

  "name": "Like Remover",
  "description": "他の人がいいねしたツイートをTLから消し去ります！",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon.png"
  },
  "version": "1.0",

  "content_scripts": [
    {
        "matches": ["https://twitter.com/*"],
        "css": ["mystyles.css"],
        "js": ["jquery.js", "myscript.js"]
    }
  ],
  
  "permissions": [
    "activeTab",
    "*://twitter.com/*",
    "*://*.twitter.com/*"
  ]
}
